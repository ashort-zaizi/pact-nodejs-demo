{
  "consumer": {
    "name": "client-service-consumer"
  },
  "provider": {
    "name": "client-service-provider"
  },
  "interactions": [
    {
      "description": "a request for all clients",
      "providerState": "i have a list of clients",
      "request": {
        "method": "GET",
        "path": "/clients",
        "headers": {
          "Accept": "application/json, text/plain, */*"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "firstName": "Lisa",
            "lastName": "Simpson",
            "age": 8,
            "id": 1
          },
          {
            "firstName": "Lisa",
            "lastName": "Simpson",
            "age": 8,
            "id": 1
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 2
          },
          "$.body[*].*": {
            "match": "type"
          },
          "$.body[*].firstName": {
            "match": "type"
          },
          "$.body[*].lastName": {
            "match": "type"
          },
          "$.body[*].age": {
            "match": "type"
          },
          "$.body[*].id": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a request to get a single client",
      "providerState": "a client with ID equal to 1 exists",
      "request": {
        "method": "GET",
        "path": "/clients/1",
        "headers": {
          "Accept": "application/json, text/plain, */*"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "firstName": "Lisa",
          "lastName": "Simpson",
          "age": 8,
          "id": 1
        },
        "matchingRules": {
          "$.body.firstName": {
            "match": "type"
          },
          "$.body.lastName": {
            "match": "type"
          },
          "$.body.age": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a request to create a new client with firstname, lastname and age",
      "providerState": "client server is available",
      "request": {
        "method": "POST",
        "path": "/clients",
        "headers": {
          "Content-Type": "application/json;charset=utf-8"
        },
        "body": {
          "firstName": "Andy",
          "lastName": "Test",
          "age": 21
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "firstName": "Andy",
          "lastName": "Test",
          "age": 21,
          "id": 3
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}